import java.util.*;
public class constover{
    int b,h,r; double length,breadth;
    double area=0;
    constover()
    {
        b=h=r=0;
        length=breadth=0.0;
    }
    constover(int x,int y)
    {
        b=x;
        h=y;
        area=0.5*b*h;
        System.out.println("Area of triangle="+area);
    }
    constover(double a,double b)
    {
     length=a;
     breadth=b;
     area=length*breadth;
        System.out.println("Area of rectangle="+area);
    }
    constover(int rad)
    {
        r=rad;
        area=3.14*r*r;
        System.out.println("Area of circle="+area);
    }
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int p=sc.nextInt();
        int q=sc.nextInt();
        double c=sc.nextDouble();
        double d=sc.nextDouble();
        int radius=sc.nextInt();
        constover ob1=new constover(p,q);
        constover ob2=new constover(c,d);
        constover ob3=new constover(radius);
    }
}
