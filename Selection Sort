#include <stdio.h>
#define SIZE 5
int a[SIZE];
void selectionsort()
{int t=0,min=0,index;

    for(int i=0;i<SIZE-1;i++)
    {
        min=a[i];//the elements gets sorted from the beginning
        for(int j=i+1;j<SIZE;j++)
        {
            if(a[j]<min)
            {
                index=j;
                min=a[j];
            }
            t=a[i];
        a[i]=a[index];
        a[index]=t;
        }

    }
}
void main()
{

    printf("Enter elements in the array\n");

    for(int i=0;i<SIZE;i++)
    {
        scanf("%d ",&a[i]);
    }
    selectionsort();
    printf("Sorted Array\n");
    for(int i=0;i<SIZE;i++)
    {
        printf("%d ",a[i]);
    }

}
