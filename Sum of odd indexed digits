/*package whatever //do not write package name here */

import java.io.*;
import java.util.*;
class GFG {
	public static void main (String[] args) {
		int n,sum=0,i=1;
		Scanner sc=new Scanner(System.in);
		n=sc.nextInt();
		while(n>0)
		{
		    int d=n%10;
		    sum=sum+d;
		    n=n/100;
		}
	    System.out.println("Sum="+sum);
	}
}
